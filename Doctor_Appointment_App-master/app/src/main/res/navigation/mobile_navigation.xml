<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myapplicationdc.Activity.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_appointments"
        android:name="com.example.myapplicationdc.Activity.ui.dashboard.DashboardFragment"
        android:label="Available Days"
        tools:layout="@layout/fragment_dashboard" />


    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.myapplicationdc.Activity.ui.ProfileFragment"
        android:label="My Profile"
        tools:layout="@layout/fragment_my_profile" />


</navigation>